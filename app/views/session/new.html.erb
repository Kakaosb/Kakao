<% provide(:title, "Вход") %>
<h1>Вход</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: session_index_path) do |f| %>

      <%= f.label :email, "Email" %>
      <%= f.text_field :email %>

      <%= f.label :password, "Пароль" %>
      <%= f.password_field :password %>

      <%= f.submit "Войти" %>
    <% end %>

    <p>New user? <%= link_to "Зарегистрироваться", signup_path %></p>
</div>
</div>